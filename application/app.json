{
  "expo": {
    "name": "Médoc",
    "slug": "mehdicament",
    "githubUrl": "https://github.com/maxgfr/mehdicament",
    "description": "Scan et recherche de médicaments",
    "privacy": "unlisted",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.3.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0B0B0B"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.maxgfr.mehdicament",
      "buildNumber": "7",
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This lets you scan QR Code and barcode for searching medication information"
      }
    },
    "android": {
      "package": "com.maxgfr.mehdicament",
      "versionCode": 7
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#161a21"
    },
    "packagerOpts": {
      "assetExts": [
        "db"
      ]
    }
  }
}
